    Top = 48
  end
  object ibqDetalle: TIBQuery
    Database = dmConexion.IBDatabase
    Transaction = dmConexion.ibTransac
    BufferChunks = 1000
    CachedUpdates = False
    DataSource = dsMaster
    SQL.Strings = (
      
        'SELECT A.CODIGO_INTERNO, A.DESCRIPCION, A.PRECIO_PUBLICO AS PREC' +
        'IO, A.STOCK, A.PUNTO_REPOSICION, A.PRECIO_PUBLICO*A.STOCK AS VAL' +
        'OR, A.FECHA_BAJA, A.CONTROL_STOCK'
      'FROM ARTICULOS A'
      'WHERE A.IDLINEA = :IDLINEA'
      'AND A.IDRUBRO = :IDRUBRO'
      'AND A.IDTITULO = :IDTITULO')
    Left = 104
    Top = 48
    ParamData = <
      item
        DataType = ftInteger
        Name = 'IDLINEA'
        ParamType = ptInput
        Size = 4
      end
      item
        DataType = ftInteger
        Name = 'IDRUBRO'
        ParamType = ptInput
        Size = 4
      end
      item
        DataType = ftInteger
        Name = 'IDTITULO'
        ParamType = ptInput
        Size = 4
      end>
    object ibqDetalleCODIGO_INTERNO: TIBStringField
      FieldName = 'CODIGO_INTERNO'
      Origin = 'ARTICULOS.CODIGO_INTERNO'
      Required = True
    end
    object ibqDetalleDESCRIPCION: TIBStringField
      FieldName = 'DESCRIPCION'
      Origin = 'ARTICULOS.DESCRIPCION'
      Required = True
      Size = 200
    end
    object ibqDetallePRECIO: TIBBCDField
      FieldName = 'PRECIO'
      Origin = 'ARTICULOS.PRECIO_PUBLICO'
      Required = True
      DisplayFormat = '$,0.##'
      EditFormat = '0.##'
      Precision = 18
      Size = 4
    end
    object ibqDetalleSTOCK: TIBBCDField
      FieldName = 'STOCK'
      Origin = 'ARTICULOS.STOCK'
      Required = True
      Precision = 9
      Size = 4
    end
    object ibqDetallePUNTO_REPOSICION: TIBBCDField
      FieldName = 'PUNTO_REPOSICION'
      Origin = 'ARTICULOS.PUNTO_REPOSICION'
      Required = True
      Precision = 9
      Size = 4
    end
    object ibqDetalleVALOR: TFloatField
      FieldName = 'VALOR'
      Required = True
      DisplayFormat = '$,0.##'
      EditFormat = ',0.##'
    end
    object ibqDetalleFECHA_BAJA: TDateField
      FieldName = 'FECHA_BAJA'
      Origin = 'ARTICULOS.FECHA_BAJA'
    end
    object ibqDetalleCONTROL_STOCK: TIntegerField
      FieldName = 'CONTROL_STOCK'
      Origin = 'ARTICULOS.CONTROL_STOCK'
      Required = True
    end
  end
  object SaveDialog: TSaveDialog
