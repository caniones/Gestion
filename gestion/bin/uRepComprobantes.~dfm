  object ibqMaster: TIBQuery
    Database = dmConexion.IBDatabase
    Transaction = dmConexion.ibTransac
    BufferChunks = 1000
    CachedUpdates = False
    SQL.Strings = (
      
        'SELECT C.ID_NROCOMPROBANTE, TC.LETRA, TC.DESCRIPCION AS TIPO_COM' +
        'PROBANTE, C.NROSUCURSAL, C.NROCOMPROBANTE, '
      
        'C.FECHA, FP.DESCRIPCION AS FORMAS_PAGOS, C.IDCLIENTE, C.TOTAL_NE' +
        'TO, C.TOTAL_IVA, C.TOTAL_DESCUENTO, C.TOTAL_GENERAL, (C.TOTAL_GE' +
        'NERAL*C.COEFICIENTE) AS TOTAL_FINANCIADO, C.OBSERVACIONES'
      'FROM COMPROBANTES C'
      
        'INNER JOIN TIPOS_COMPROBANTES TC ON TC.IDTIPOCOMPROBANTE = C.IDT' +
        'IPOCOMPROBANTE'
      'INNER JOIN FORMAS_PAGOS FP ON FP.IDFORMAPAGO = C.IDFORMAPAGO'
      'WHERE C.ID_NROCOMPROBANTE = :ID')
