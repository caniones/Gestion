  object ibqCliente: TIBQuery
    Database = dmConexion.IBDatabase
    Transaction = dmConexion.ibTransac
    BufferChunks = 1000
    CachedUpdates = False
    DataSource = dsDetalle
    SQL.Strings = (
      
        'SELECT C.NOMBRE, S.DESCRIPCION AS SITUACION, C.CUIT, C.DIRECCION' +
        ', C.CODIGO_POSTAL, L.NOMBRE||'#39' - '#39'||P.NOMBRE AS LOCALIDAD'
      'FROM CLIENTES C, LOCALIDADES L , PROVINCIAS P, SITUACIONES S'
      'WHERE C.IDCLIENTE = :IDCLIENTE'
      'AND C.IDLOCALIDAD = L.IDLOCALIDAD'
      'AND C.IDSITUACION = S.IDSITUACION'
      'AND L.IDPROVINCIA = P.IDPROVINCIA'
      'AND C.FECHA_BAJA IS NULL'
      'ORDER BY C.NOMBRE')
    Left = 48
    Top = 248
    ParamData = <
      item
        DataType = ftInteger
        Name = 'IDCLIENTE'
