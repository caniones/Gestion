  object ibqDetalle: TIBQuery
    Database = dmConexion.IBDatabase
    Transaction = dmConexion.ibTransac
    BufferChunks = 1000
    CachedUpdates = False
    DataSource = dsDetalle
    SQL.Strings = (
      
        'SELECT I.ID_NROCOMPROBANTE, A.DESCRIPCION, A.CODIGO_INTERNO, I.C' +
        'ANTIDAD, I.PRECIO, I.DESCUENTO,'
      '((I.CANTIDAD*I.PRECIO)*C.COEFICIENTE)-(I.DESCUENTO) AS SUBTOTAL'
      'FROM ITEMS I, ARTICULOS A, COMPROBANTES C'
      'WHERE I.IDARTICULO = A.IDARTICULO'
      'AND I.ID_NROCOMPROBANTE = C.ID_NROCOMPROBANTE'
      'AND I.ID_NROCOMPROBANTE = :ID_NROCOMPROBANTE')
    Left = 16
    Top = 248
    ParamData = <
      item
        DataType = ftInteger
        Name = 'ID_NROCOMPROBANTE'
        ParamType = ptInput
        Size = 4
      end>
