      DisplayFormat = 'dd/mm/yy'
    end
    object ibqDetalleFECHA_BAJA: TDateField
      FieldName = 'FECHA_BAJA'
      Origin = 'ARTICULOS.FECHA_BAJA'
    end
  end
  object ibqMaster: TIBQuery
    Database = dmConexion.IBDatabase
    Transaction = dmConexion.ibTransac
    Active = True
    BufferChunks = 1000
    CachedUpdates = False
    SQL.Strings = (
      
        'SELECT DISTINCT T.DESCRIPCION AS TITULO, L.DESCRIPCION AS LINEA,' +
        ' R.DESCRIPCION AS RUBRO, A.IDTITULO, A.IDLINEA, A.IDRUBRO'
      'FROM ARTICULOS A, LINEAS L, RUBROS R, TITULOS T'
      'WHERE A.IDLINEA = L.IDLINEA'
      'AND A.IDRUBRO = R.IDRUBRO'
      'AND A.IDTITULO = T.IDTITULO')
    Left = 48
    Top = 16
    object ibqMasterTITULO: TIBStringField
      FieldName = 'TITULO'
      Origin = 'TITULOS.DESCRIPCION'
      Required = True
      Size = 50
    end
    object ibqMasterLINEA: TIBStringField
      FieldName = 'LINEA'
      Origin = 'LINEAS.DESCRIPCION'
      Required = True
      Size = 50
    end
    object ibqMasterRUBRO: TIBStringField
      FieldName = 'RUBRO'
      Origin = 'RUBROS.DESCRIPCION'
      Required = True
      Size = 50
    end
    object ibqMasterIDTITULO: TIntegerField
      FieldName = 'IDTITULO'
      Origin = 'ARTICULOS.IDTITULO'
      Required = True
    end
    object ibqMasterIDLINEA: TIntegerField
      FieldName = 'IDLINEA'
      Origin = 'ARTICULOS.IDLINEA'
      Required = True
    end
    object ibqMasterIDRUBRO: TIntegerField
      FieldName = 'IDRUBRO'
      Origin = 'ARTICULOS.IDRUBRO'
      Required = True
    end
  end
