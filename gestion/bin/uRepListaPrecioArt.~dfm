  object ibqDetalle: TIBQuery
    Database = dmConexion.IBDatabase
    Transaction = dmConexion.ibTransac
    Active = True
    BufferChunks = 1000
    CachedUpdates = False
    DataSource = dsMaster
    SQL.Strings = (
      
        'SELECT A.CODIGO_INTERNO, A.DESCRIPCION, A.PRECIO_PUBLICO, A.FECH' +
        'A_ACTPRECIO AS FECHA, A.FECHA_BAJA'
      'FROM ARTICULOS A'
      'WHERE A.IDLINEA = :IDLINEA'
      'AND A.IDRUBRO = :IDRUBRO'
      'AND A.IDTITULO = :IDTITULO')
    Left = 115
    Top = 16
    ParamData = <
      item
        DataType = ftInteger
        Name = 'IDLINEA'
        ParamType = ptUnknown
        Size = 4
      end
      item
        DataType = ftInteger
        Name = 'IDRUBRO'
        ParamType = ptUnknown
        Size = 4
      end
      item
        DataType = ftInteger
        Name = 'IDTITULO'
        ParamType = ptUnknown
        Size = 4
      end>
    object ibqDetalleCODIGO_INTERNO: TIBStringField
      FieldName = 'CODIGO_INTERNO'
      Origin = 'ARTICULOS.CODIGO_INTERNO'
      Required = True
    end
    object ibqDetalleDESCRIPCION: TIBStringField
      FieldName = 'DESCRIPCION'
      Origin = 'ARTICULOS.DESCRIPCION'
      Required = True
      Size = 50
    end
    object ibqDetallePRECIO_PUBLICO: TIBBCDField
      FieldName = 'PRECIO_PUBLICO'
      Origin = 'ARTICULOS.PRECIO_PUBLICO'
      Required = True
      DisplayFormat = '$,0.##'
      EditFormat = '0.##'
      Precision = 18
      Size = 4
    end
    object ibqDetalleFECHA: TDateField
      FieldName = 'FECHA'
