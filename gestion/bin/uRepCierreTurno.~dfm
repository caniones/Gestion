  object ibqTotales: TIBQuery
    Database = dmConexion.IBDatabase
    Transaction = dmConexion.ibTransac
    BufferChunks = 1000
    CachedUpdates = False
    SQL.Strings = (
      
        'SELECT SUM(PSCONTADO) AS CONTADO, SUM(PSDEBITO) AS DEBITO, SUM(P' +
        'SCREDITO) AS CREDITO, SUM(PSCHEQUE) AS CHEQUE, SUM(PSCUENTACORRI' +
        'ENTE) AS CUENTA, SUM(PSTICKET) AS TICKET'
      'FROM SP_CIERRE_TURNO(:IDCIERRE)'
      'WHERE PSIDTIPOCOMPROBANTE NOT IN (200)')
    Left = 40
    Top = 240
    ParamData = <
      item
        DataType = ftInteger
        Name = 'IDCIERRE'
        ParamType = ptInput
      end>
