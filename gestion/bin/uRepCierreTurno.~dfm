  object ibqDetalle: TIBQuery
    Database = dmConexion.IBDatabase
    Transaction = dmConexion.ibTransac
    BufferChunks = 1000
    CachedUpdates = False
    SQL.Strings = (
      
        'SELECT PSDESCRIPCION AS DESCRIPCION, SUM(PSCONTADO) AS CONTADO, ' +
        'SUM(PSDEBITO) AS DEBITO, SUM(PSCREDITO) AS CREDITO, SUM(PSCHEQUE' +
        ') AS CHEQUE, SUM(PSCUENTACORRIENTE) AS CUENTA, SUM(PSTICKET) AS ' +
        'TICKET'
      'FROM SP_CIERRE_TURNO(:IDCIERRE)'
      'WHERE PSIDTIPOCOMPROBANTE NOT IN (200)'
      'GROUP BY 1'
      'ORDER BY 1 DESC')
    Left = 40
    Top = 192
    ParamData = <
      item
        DataType = ftInteger
        Name = 'IDCIERRE'
        ParamType = ptInput
      end>
    object ibqDetalleDESCRIPCION: TIBStringField
      FieldName = 'DESCRIPCION'
